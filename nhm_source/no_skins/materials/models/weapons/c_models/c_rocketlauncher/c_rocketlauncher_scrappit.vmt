"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_models/c_rocketlauncher/c_rocketlauncher"
	"$phongexponenttexture" "models/weapons/c_models/c_rocketlauncher/c_rocketlauncher_exp"
	
	"$phong" "1"
	//commented out to use the exponent mask
	//"$phongexponent" "255"
	"$phongboost" "2.5"	
	
	//scales up the exponent texture which usually only has about 120 levels of exponent
	"$phongexponentfactor"  "255"
	
	"$lightwarptexture" "models/lightwarps/softened_weapon_lightwarp"
			
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	"$glowcolor" "1"
	
	"$envmap" "env_cubemap"
	"$envmaptint" "[.15 .15 .15]"

	"$tempvar" "0"

	//commented out to use the exponent mask
	//"$basephongexponent" "15"
	//"$basephongboost" "3"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "2"				
	"$rimlightboost" "5"
	"$rimmask" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
    "Empty"
    {
		"WeaponSkin"
    {
		}
    }    
	}
}