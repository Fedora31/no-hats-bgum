"VertexLitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_models/c_demo_sultan_sword/c_demo_sultan_sword"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "2.5"	
	
	"$phongfresnelranges"	"[.25 .5 20]"
	"$halflambert" "1"

	"$basemapalphaphongmask" "1"
	
	// Rim lighting parameters
	"$rimlight" "1"					
	"$rimlightexponent" "4"		
	"$rimlightboost" "2"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
    "Empty"
    {
		"WeaponSkin"
    {
		}
    }    
	}
}