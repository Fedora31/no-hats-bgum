"VertexLitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_trenchgun/c_trenchgun.vtf"

	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"	

    	"$phong"                "1"
    	"$phongexponent"        "17"
    	"$phongboost"           "9"
  	"$phongfresnelranges"   "[.25 .5 1]"
	"$halflambert" "1"

	"$commadd_phongexponent" "20"
	"$commadd_phongboost" 	"10"
	"$commweapon"		"0"

	// Rim lighting parameters
	"$rimlight" "1"			
	"$rimlightexponent" "4"	
	"$rimlightboost" "1"

	"$basemapalphaphongmask" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"


	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
    "Empty"
    {
		"WeaponSkin"
    {
		}
    }    
	}
}
